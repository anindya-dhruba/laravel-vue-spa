        "jquery": "^3.3.1",
        "laravel-mix": "^2.0",
        "popper.js": "^1.14.4",
        "vue": "^2.5.17",
        "vue-router": "^3.0.1",
        "vuejs-noty": "^0.1.3",
        "vuex": "^3.0.1"
    },
